<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.grocerygogetters.model.Feedback" table="Feedback">
		<id name="feedback_id" column="FEEDBACK_ID">
			<generator class="increment"></generator>
		</id>
		<property name="feedback_rating" type="org.hibernate.type.StringType" column="FEEDBACK_RATING"></property>
		<property name="feedback_message" type="org.hibernate.type.StringType" column="FEEDBACK_MESSAGE"></property>
	</class>


</hibernate-mapping>