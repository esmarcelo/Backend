public class Driver {

	public static void main(String[] args) {
//		Session s = HibernateUtil.getSession();
//		s.close();
		
		UsersDao user = new UsersDaoImpl();
		Users u = new Users();
		u.setUser_fname("John");
		u.setUser_lname("Smith");
		u.setUser_email("jsmith@email.com");
		u.setUser_password("1234");
