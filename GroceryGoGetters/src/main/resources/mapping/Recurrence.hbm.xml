<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.grocerygogetters.model.Recurrence" table="Recurrence">
		<id name="recur_id" column="RECUR_ID">
			<generator class="increment"></generator>
		</id>
		<property name="recur_type" type="org.hibernate.type.StringType" column="RECUR_TYPE"></property>
		<property name="recur_descr" type="org.hibernate.type.StringType" column="RECUR_DESCR"></property>
				
	</class>

</hibernate-mapping>